{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "description": "Google's stock price over time.",
  "width": 700,
  "height": 300,
  "data": {
    "url": "https://raw.githubusercontent.com/dtan0032/FIT3179_A2/main/data/master_updated.csv"
  },
  "transform": [
    {
      "aggregate": [
        {"op": "sum", "field": "suicides/100k pop", "as": "Suicides /100k Population"}
      ],
      "groupby": ["year"]
    }
  ],
  "layer":[{
    "mark": {"type":"line","point": {
      "filled": false,
      "fill": "black"},
      "tooltip":true},
    "encoding": {
      "x": {"field": "year",
        "type": "ordinal",
        "axis": {
          "grid": false,
          "labelAngle":90
          },
          "title":"Year"
        },
      "y": {"field": "Suicides /100k Population", 
        "type": "quantitative",
        "axis": {"grid": false}},
      "color":{"value":"black"}}
    },
    {
      "mark": {
        "type": "text",
        "align": "left",
        "dx": -80,
        "dy": -80,
        "text": "1995",
        "color": "indianred",
        "fontSize": 15
      }
    },
    {
      "mark": {
        "type": "text",
        "align": "left",
        "dx": -120,
        "dy": -81,
        "text": "Year :",
        "color": "black",
        "fontSize": 15
      }
    },
    {
      "mark": {
        "type": "text",
        "align": "left",
        "dx": 10,
        "dy": -65,
        "text": "14660",
        "color": "indianred",
        "fontSize": 15
      }
    },
    {
      "mark": {
        "type": "text",
        "align": "left",
        "dx": -120,
        "dy": -66,
        "text": "Suicides/100K Pop: ",
        "color": "black",
        "fontSize": 15
      }
    },
    


    {
      "mark": {
        "type": "text",
        "align": "right",
        "dx": 305,
        "dy": 0,
        "text": "2015",
        "color": "#e4af45",
        "fontSize": 15
      }
    },
    {
      "mark": {
        "type": "text",
        "align": "right",
        "dx": 269,
        "dy": 0,
        "text": "Year :",
        "color": "black",
        "fontSize": 15
      }
    },
    {
      "mark": {
        "type": "text",
        "align": "right",
        "dx": 305,
        "dy": 16,
