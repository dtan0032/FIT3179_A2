{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "description": "Stock prices of 5 Tech Companies over Time.",
  "data": {
    "url": "https://raw.githubusercontent.com/dtan0032/FIT3179_A2/main/data/master_updated.csv"
  },
  "height": 300,
  "width": 600,
  "layer": [
    {
      "mark": {"type": "line", "point": true, "tooltip": true},
      "encoding": {
        "x": {"field": "year", "type": "ordinal", "title": "Year"},
        "y": {
          "aggregate": "sum",
          "field": "suicides_no",
          "type": "quantitative",
          "title": "Sum of suicides per year",
          "axis":{"grid":false}
        },
        "color": {"field": "age", "type": "nominal"}
      }
    },
    {
      "mark": {
        "type": "text",
        "align": "left",
        "dx": -39,
        "dy": -80,
        "text": "35-54 years old ",
        "color": "indianred",
        "fontSize": 15
      }
    },
    {
      "mark": {
        "type": "text",
        "align": "left",
        "dx": -130,
        "dy": -80,
        "text": "Age group of ",
        "color": "black",
        "fontSize": 15
      }
    },
    {
      "mark": {
        "type": "text",
        "align": "left",
        "dx": 67,
        "dy": -80,
        "text": " were consistently ",
        "color": "black",
        "fontSize": 15
      }
    },
    {
      "mark": {
        "type": "text",
        "align": "left",
        "dx": 40,
        "dy": -65,
        "text": "higher than all other age groups.",
        "color": "black",
        "fontSize": 15
      }
    }
  ],
  "config": {}
}
